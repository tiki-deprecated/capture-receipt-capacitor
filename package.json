{
  "name": "@mytiki/tiki-capture-receipt-capacitor",
  "version": "0.1.0",
  "description": "Capture receipts w/ TIKI!",
  "main": "dist/plugin.cjs.js",
  "module": "dist/esm/index.js",
  "type": "module",
  "types": "dist/esm/index.d.ts",
  "unpkg": "dist/plugin.js",
  "files": [
    "android/src/main/",
    "android/build.gradle",
    "dist/",
    "ios/Plugin/",
    "MytikiTikiCaptureReceiptCapacitor.podspec"
  ],
  "author": "Tiki inc.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tiki-bar/tiki-capture-receipt-capacitor.git"
  },
  "bugs": {
    "url": "https://github.com/tiki-bar/tiki-capture-receipt-capacitor/issues"
  },
  "keywords": [
    "capacitor",
    "plugin",
    "tiki",
    "microblink"
  ],
  "scripts": {
    "verify": "npm run verify:ios && npm run verify:android && npm run verify:web",
    "verify:ios": "cd ios && pod install && xcodebuild -workspace Plugin.xcworkspace -scheme Plugin -destination generic/platform=iOS && cd ..",
    "verify:android": "cd android && ./gradlew clean build test && cd ..",
    "verify:web": "npm run build",
    "lint": "npm run eslint && npm run prettier -- --check && npm run swiftlint -- lint",
    "fmt": "npm run eslint -- --fix && npm run prettier -- --write && npm run swiftlint -- --fix --format",
    "eslint": "eslint plugin --ext ts",
    "prettier": "prettier \"**/*.{css,html,ts,js,java}\"",
    "swiftlint": "node-swiftlint",
    "build": "npm run clean && tsc && rollup -c rollup.config.js",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@capacitor/android": "^5.2.2",
    "@capacitor/core": "^5.2.2",
    "@capacitor/docgen": "^0.2.1",
    "@capacitor/ios": "^5.2.2",
    "@ionic/eslint-config": "^0.3.0",
    "@ionic/prettier-config": "^4.0.0",
    "@ionic/swiftlint-config": "^1.1.2",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "eslint": "^7.32.0",
    "prettier": "~3.0.1",
    "prettier-plugin-java": "~2.2.0",
    "rimraf": "^5.0.1",
    "rollup": "^3.28.0",
    "swiftlint": "^1.0.2",
    "typescript": "~5.1.6"
  },
  "peerDependencies": {
    "@capacitor/core": "^5.0.0"
  },
  "prettier": "@ionic/prettier-config",
  "swiftlint": "@ionic/swiftlint-config",
  "eslintConfig": {
    "extends": "@ionic/eslint-config/recommended"
  },
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  }
}
